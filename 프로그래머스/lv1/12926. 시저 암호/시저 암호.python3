def solution(s, n):
    ans = ''
    tmp = []
    #인고딩
    # 영어 알파벳은 26개
    for i in s:
        if 65 <= ord(i) <= 90:
            # 아스키 코드 구하기
            ans += chr( (( (ord(i)+n)-65) %26 ) +65)
        
        elif 97 <= ord(i) <= 122:
            # 아스키 코드 구하기 
            ans += chr( (( (ord(i)+n)-97) %26) +97)

        else:
            ans += ' '

        
            

    
    return ans